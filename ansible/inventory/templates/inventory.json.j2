{
    "_meta": {
        "hostvars": {
            "vm-dev": {
                "ansible_host": "{{ inventory.dev.vm.ip }}",
                "ansible_user": "{{ inventory.dev.vm.user }}",
                "ansible_ssh_private_key_file": "~/.ssh/id_rsa",
                "ansible_python_interpreter": "/usr/bin/python3"
            },
            "vm-prod": {
                "ansible_host": "{{ inventory.prod.vm.ip }}",
                "ansible_user": "{{ inventory.prod.vm.user }}",
                "ansible_ssh_private_key_file": "~/.ssh/id_rsa",
                "ansible_python_interpreter": "/usr/bin/python3"
            }
        }
    },
    "dev": {
        "hosts": [
            "vm-dev"
        ]
    },
    "prod": {
        "hosts": [
            "vm-prod"
        ]
    },
    "all": {
        "children": [
            "dev",
            "prod"
        ]
    }
}